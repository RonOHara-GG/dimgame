<Screen>
	<Name=LoginScreen></Name>
	<BackgroundTexture=LoadingBackground.dds></BackgroundTexture>
	<Window>
		<Name=LoginDialog></Name>
		<BackgroundTexture=DefaultBackground.dds></BackgroundTexture>
		<FrameTexture=DefaultFrame.dds></FrameTexture>
		<FrameCornerTexture=DefaultFrameCorner.dds></FrameCornerTexture>
		<XPos=Center></XPos>
		<YPos=Center></YPos>
		<Width=0.25></Width>
		<Height=0.65></Height>
		<Interactable=false></Interactable>
		
		<StaticText>
			<Text="Account Name"></Text>
			<Font="Arial"></Font>
			<XPos=0.125f></XPos>
			<YPos=0.25f></YPos>
		</StaticText>
		
		<TextBox>
			<Name=AccountName></Name>
			<XPos=0.4f></XPos>
			<YPos=0.25f></YPos>
		</TextBox>
		
		<StaticText>
			<Text="Password"></Text>
			<Font="Arial></Font>
			<XPos=0.125f></XPos>
			<YPos=0.45f></YPos>
		</StaticText>
		
		<TextBox>
			<Password=true></Password>
			<Name=Password></Name>
			<Xpos=0.4f></Xpos>
			<Ypos=0.45f></Ypos>
		</TextBox>
		
		<Button>
			<Name=Progress0></Name>
			<Text="Login"></Text>
			<BackgroundTexture=DefaultBackground.dds></BackgroundTexture>
			<FrameTexture=DefaultFrame.dds></FrameTexture>
			<FrameCornerTexture=DefaultFrameCorner.dds></FrameCornerTexture>
			<Xpos=0.2f></Xpos>
			<Ypos=0.9f></Ypos>
		</Button>
		
		<Button>
			<Name=Regress0></Name>
			<Text="Exit"></Text>
			<BackgroundTexture=DefaultBackground.dds></BackgroundTexture>
			<FrameTexture=DefaultFrame.dds></FrameTexture>
			<FrameCornerTexture=DefaultFrameCorner.dds></FrameCornerTexture>
			<Xpos=0.4f></Xpos>
			<Ypos=0.9f></Ypos>
		</Button>
	</Window>
</Screen>